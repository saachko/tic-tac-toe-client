{
  "endOfLine": "auto",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 80,
  "arrowParens": "always",
  "bracketSameLine": false,
  "importOrder": [
    "^react/(.*)$",
    "^(redux/(.*)$)",
    "^(pages/(.*)$)",
    "^(components/(.*)$)",
    "^(App)",
    "^(utils/(.*)$)",
    "^(hooks/(.*)$)",
    "^(ts/(.*)$)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
